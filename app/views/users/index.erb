<%# app/views/users/index.erb %>

<table>
<tr>
	<th>ID</th>
	<th>First</th>
	<th>Last</th>
	<th>Username</th>
	<th>Action</th>
</tr>
<% @users.each do |user| %>
	<tr>
		<td><a href="/users/<%= user.id %>"><%= user.id %></a></td>
		<td><%= user.fname %></td>
		<td><%= user.lname %></td>
		<td><%= user.username %></td>
		<td><%= link_to "Delete", "/users/#{user.id}", method: :delete %></td>
	</tr>
<% end %>
</table>